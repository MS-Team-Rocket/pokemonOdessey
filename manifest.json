{
    "name": "Pokemon Catcher",
    "version": "0.0.1",
    "description": "Pokemon game for educational purposes",
    "manifest_version": 3,
    "action":{
        "default_icon": "pokeBall.png",
        "default_popup": "popup.html"
    },
    "web_accessible_resources": [{
        "resources": ["assets/*", "popup.css"],
        "matches": ["<all_urls>"]
    }],
    "background":{
        "service_worker": "background.js"
    },
    "permissions": ["tabs"]
}